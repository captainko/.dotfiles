	{ emulators, firefox, firecamp, ebook-viewer --continue, blender, code, bluetooth_switch, mpv_last }
super + { bracketleft, bracketright, backslash }
	playerctl { previous, next, play-pause }
super + a ; { a, s, d }
	{ switch_audio_output, switch_app_audio_output, switch_default_audio_output }
